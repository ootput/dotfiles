[user]
	name = Joseph LP
	email = ootput@gmail.com
[color]
        branch = auto
        diff = auto
        interactive = auto
        status = auto
	ui = auto
[format]
        pretty = oneline
[pack]
        threads = 0
[alias]
        br = branch
        ci = commit
        co = checkout
        di = diff
        llog = log --pretty=oneline --abbrev-commit
        st = status
        sta = stash
        last = cat-file commit HEAD
	d = difftool
[core]
	editor = "vim"
	whitespace = trailing-space,space-before-tab
[advice]
	pushNonFastForward = false
	statusHints = false
[diff]
	renames = copies
	mnemonicprefix = true
	tool = vimdiff
[branch]
	autosetupmerge = true
[push]
	default = tracking
[rerere]
	enabled = true
[merge]
	stat = true
[difftool]
	prompt = false
